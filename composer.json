{
	"name": "stanislav-janu/browser",
	"description": "Browser checker component",
	"license": "MIT",
	"authors": [
		{
			"name": "Stanislav JanÅ¯",
			"homepage": "https://www.lweb.cz"
		}
	],
	"require": {
		"php": ">=7.2.0",
		"nette/utils": "^2.5 || ^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^7",
		"stanislav-janu/phpstan": "^0.12.0"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"scripts": {
		"sa": "vendor/bin/phpstan.phar analyse --ansi --pro"
	},
	"minimum-stability": "stable",
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"config": {
		"platform": {
			"php": "7.2"
		},
		"process-timeout": 0
	}
}
